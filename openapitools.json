{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "generator-cli": {
        "useDocker": true,
        "version": "6.6.0",
        "generators": {
            "medication": {
                "generatorName": "typescript-axios",
                "glob": "api/medication.openapi.yaml",
                "output": "#{cwd}/src/api/medication",
                "additionalProperties": {
                    "supportsES6": "true",
                    "withInterfaces": true,
                    "enablePostProcessFile": true
                }
            }
        }
    }
 }